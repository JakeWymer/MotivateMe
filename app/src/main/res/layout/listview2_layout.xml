<?xml version="1.0" encoding="utf-8"?>

<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="vertical" android:layout_width="match_parent"
    android:layout_height="match_parent">

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceMedium"
        android:text="Med Text"
        android:id="@+id/list_log"
        android:layout_alignParentTop="true"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_marginTop="61dp"/>

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceMedium"
        android:text="Med Text"
        android:id="@+id/list_size"
        android:layout_alignBottom="@+id/list_log"
        android:layout_toRightOf="@+id/list_log"
        android:layout_marginLeft="10dp"/>

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceMedium"
        android:text="Med Text"
        android:id="@+id/list_diet"
        android:layout_alignBottom="@+id/list_size"
        android:layout_toRightOf="@+id/list_size"
        android:layout_marginLeft="30dp"/>

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceMedium"
        android:text="Med Text"
        android:id="@+id/list_wo"
        android:layout_alignBottom="@+id/list_diet"
        android:layout_toRightOf="@+id/list_diet"
        android:layout_marginLeft="40dp"/>

</RelativeLayout>